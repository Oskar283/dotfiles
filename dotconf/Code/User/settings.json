{
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": "off"
    },
    "dart.sdkPath": "~/Android/Sdk",
    "http.proxyStrictSSL": false,
    "gitlens.proxy": {},
    "hediet.vscode-drawio.resizeImages": null,
    "plantuml.render": "Local",
    "hediet.vscode-drawio.theme": "min",
    "hediet.vscode-drawio.codeLinkActivated": false,
    "plantuml.server": "http://localhost:8080",
    "files.autoSaveDelay": 10000000000000000000,
    "files.refactoring.autoSave": false,
    "C_Cpp.codeAnalysis.runAutomatically": false,
    "debug.saveBeforeStart": "none",
    "task.saveBeforeRun": "never",
    "cmake.saveBeforeBuild": false,
    "latex-workshop.message.warning.show": false,
    "gitlens.codeLens.enabled": false,
    "vim.handleKeys": {
        "<C-d>": true,
        "<C-s>": false,
        "<C-z>": false
    },
    "security.workspace.trust.untrustedFiles": "open",
    "markdown-preview-enhanced.plantumlServer": "http://localhost:8080",
    "markdown-preview-enhanced.liveUpdate": false,
    "explorer.confirmDelete": false,
    "cmake.showOptionsMovedNotification": false,
    "git.blame.editorDecoration.enabled": true,
    "git.openRepositoryInParentFolders": "always",
    "chat.editing.confirmEditRequestRetry": false,
    "window.titleBarStyle": "native",
    "python.analysis.indexing": false,
    "debug.onTaskErrors": "abort",
    "github.copilot.nextEditSuggestions.enabled": true,
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.workspaceParsingPriority": "low",
    "C_Cpp.default.configurationProvider": "compile_commands.json"
}